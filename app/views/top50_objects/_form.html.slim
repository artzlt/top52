div class="row"
  div class="col-xs-8"
    = bootstrap_form_for @top50_object do |f|
      = f.text_field :id, disabled: true
      = f.collection_select :type_id, Top50ObjectType.all, :id, :name
      = f.collection_select :is_valid, Top50ValidType.all, :id, :name, {include_blank: true, label: "Статус"}
      = f.form_group do
        = f.submit t("buttons.save", default: "Save")
