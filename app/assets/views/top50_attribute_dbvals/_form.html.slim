div class="row"
  div class="col-xs-8"
    = bootstrap_form_for @top50_attribute_dbval do |f|
      = f.fields_for @top50_attribute do |f1|
        = f1.text_field :name
        = f1.text_field :name_eng
        = f1.select :attr_type, [['Attribute-value', 1]] 
      = f.fields_for @top50_attribute_dbval do |f2| 
        =f2.collection_select :datatype_id, Top50AttributeDatatype.all, :id, :db_code 
      = f.form_group do
        = f.submit t("buttons.save", default: "Save")
